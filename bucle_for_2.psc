//crear programa de tabla de multiplicar con bucle for
Algoritmo bucle_for_2
	Definir f , tabla Como Entero;
	Escribir 'ingrese la tabla...';
	leer tabla;
	Escribir 'LA TABLA DEL ', tabla;
	Escribir '=======================';
	para f = 1 hasta 10 Hacer
		Escribir  tabla ,' x ',f,' = ',tabla * f;
	FinPara
	
	
FinAlgoritmo
